<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.candra.moneyapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:name=".di.MyApplication"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyApp"
        tools:targetApi="31">
        <activity
            android:name=".ui.activity.main.MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.edit.EditDataBudgetMonthlyActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.edit.EditActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.main.VillageActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.main.SubdistrictActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.main.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>